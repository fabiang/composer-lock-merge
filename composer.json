{
    "name": "fabiang/composer-lock-merge",
    "description": "Composer plugin that lets you easily merge composer.lock files",
    "homepage": "http://github.com/fabiang/lock-merge",
    "type": "application",
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "^2.6",
        "composer/composer": "@dev",
        "jelix/inifile": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "satooshi/php-coveralls": "^0.6.1",
        "codeclimate/php-test-reporter": "@dev"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "f.grutschus@lubyte.de",
            "homepage": "http://www.lubyte.de/"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Fabiang\\ComposerLockMerge\\": "src/"
        }
    }
}
